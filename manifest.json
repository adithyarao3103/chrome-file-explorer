{
    "manifest_version": 3,
    "name": "File Explorer",
    "version": "1.0",
    "description": "Enhances the appearance of Chrome's local file explorer with modern design",
    "permissions": ["activeTab"],
    "content_scripts": [
        {
            "matches": ["file:///*"],
            "css": ["styles.css"],
            "js": ["content.js"]
        }
    ]
}